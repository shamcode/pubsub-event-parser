{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [ "jsdoc","closure" ]
  },
  "source": {
    "include": [ "./" ],
    "exclude": [ "./node_modules" ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "opts": {
    "recurse": true,
    "destination": "./docs/"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}